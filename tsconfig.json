{
	"compilerOptions": {
		"module": "commonjs",
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"outDir": "dist",
		"sourceMap": true,
		"resolveJsonModule": true,
		"strict": true,
		"esModuleInterop": true,
		"target": "es2017"
	},
	"compileOnSave": true,
	"include": ["src", "@types", "src/**/*.json"]
}
